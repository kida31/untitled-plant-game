; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[addons]

resources_spreadsheet_view/array_color_tint=100.0
resources_spreadsheet_view/color_rows=true
resources_spreadsheet_view/array_min_width=128.0
resources_spreadsheet_view/resource_preview_size=32.0
resources_spreadsheet_view/clip_headers=false
resources_spreadsheet_view/dupe_arrays=true
resources_spreadsheet_view/context_menu_on_leftclick=false

[application]

config/name="untitled-plant-game"
config/version="0.1.2"
run/main_scene="res://Main.tscn"
config/features=PackedStringArray("4.3", "C#", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

GameStateMachine="*res://Common/GameStates/GameStateMachine.cs"
EventBus="*res://Common/EventBus.cs"
TimeController="*res://Common/TimeController.cs"
WeatherCycle="*res://Features/Cycle/Weather/WeatherCycle.cs"
InputRemapper="*res://Common/Inputs/InputRemapper.cs"
InteractionManager="*res://Features/Interaction/InteractionManager.tscn"
PlantDatabase="*res://Features/Plants/PlantDatabase.cs"
DialogueDatabase="*res://Features/Dialogue/DialogueDatabase.cs"
CollisionManager="*res://Features/Player/CollisionManager.cs"
DialogueSystem="*res://Features/Dialogue/DialogueSystem.cs"
CurrencyFaithOfficer="*res://Features/Systems/CurrencyFaithOfficer.cs"
SceneManager="*res://Features/SceneManager/SceneManager.cs"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="untitled-plant-game"

[editor_plugins]

enabled=PackedStringArray("res://addons/resources_spreadsheet_view/plugin.cfg")

[global_group]

player=""
Interactables=""

[gui]

theme/custom="res://Assets/DefaultUPGTheme.tres"

