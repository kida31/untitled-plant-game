[sub_resource type="Resource" id="Resource_yfdhw"]
script = ExtResource("1_y7onw")
speakerName = "Laya"
dialogueText = "Huh... Where am I? What is this place?"

[sub_resource type="Resource" id="Resource_a8n2g"]
script = ExtResource("1_y7onw")
speakerName = "Laya"
dialogueText = "I feel like I have been here before but something feels off... Oh no!! "

[sub_resource type="Resource" id="Resource_4ymo2"]
script = ExtResource("1_y7onw")
speakerName = "Laya"
dialogueText = "This place was swallowed by the floods! No wonder I haven't heard any prayers. They must have forgotten about me."

[sub_resource type="Resource" id="Resource_da23w"]
script = ExtResource("1_y7onw")
speakerName = "Laya"
dialogueText = "It looks as if misery has not bypassed these people here. Their faces bear the marks of hardship, their shoulders are hunched under the weight of a life that has not been merciful."

[sub_resource type="Resource" id="Resource_w0ss3"]
script = ExtResource("1_y7onw")
speakerName = "Laya"
dialogueText = "So many residents are wounded and sick! I should aid the people."

[sub_resource type="Resource" id="Resource_g4jrr"]
script = ExtResource("1_y7onw")
speakerName = "Laya"
dialogueText = "Maybe the old man over there can give me some guidance."

